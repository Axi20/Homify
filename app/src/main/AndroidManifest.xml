<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Home_inventory_v2"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CategoryCleaningActivity"/>
        <activity android:name=".CategoryFoodActivity"/>
        <activity android:name=".CategoryHomeActivity"/>
        <activity android:name=".CategoryHygieneActivity"/>
        <activity android:name=".CategoryRecipesActivity"/>
        <activity android:name=".SettingsActivity"/>
        <activity android:name=".ShoppingListActivity"/>
        <activity android:name=".AddItemActivity"/>
    </application>

</manifest>